    "paths": {
        "/api/artists": {
            "get": {
                "tags": [
                    "Artists"
                ],
                "summary": "Display a listing of the Artists.",
                "description": "Displays all the Artists",
                "operationId": "2c8aed1387cca1c65d98da7ee11ac926",
                "responses": {
                    "200": {
                        "description": "Successful operation, Returns a list of Artists in JSON format"
                    },
                    "401": {
                        "description": "Unauthenticated"
                    },
                    "403": {
                        "description": "Forbidden"
                    }
                }
            }
        },
        "/api/artists/{id}": {
            "get": {
                "tags": [
                    "Artists"
                ],
                "summary": "Display the specified Artists by {id}.",
                "description": "Gets an Artist by ID",
                "operationId": "d0b68c06b8bc310fbe24c6d3ea80dac3",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "Artist ID",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful operation"
                    },
                    "401": {
                        "description": "Unauthenticated"
                    },
                    "403": {
                        "description": "Forbidden"
                    }
                }
            }
        },
        "/api/genres": {
            "get": {
                "tags": [
                    "Genres"
                ],
                "summary": "Display a listing of the generes.",
                "description": "Displays all the genres",
                "operationId": "eb12635064fefadf7ff7e54900a73534",
                "responses": {
                    "200": {
                        "description": "Successful operation, Returns a list of genres in JSON format"
                    },
                    "401": {
                        "description": "Unauthenticated"
                    },
                    "403": {
                        "description": "Forbidden"
                    }
                }
            }
        },
        "/api/genres/{id}": {
            "get": {
                "tags": [
                    "Genres"
                ],
                "summary": "Display the specified genres by {id}.",
                "description": "Gets an genre by ID",
                "operationId": "3e2ac2805d9f25d984bab075133bbb96",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "Genre ID",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful operation"
                    },
                    "401": {
                        "description": "Unauthenticated"
                    },
                    "403": {
                        "description": "Forbidden"
                    }
                }
            }
        },
        "/api/musics": {
            "get": {
                "tags": [
                    "Songs"
                ],
                "summary": "Display a listing of the songs.",
                "description": "Displays all the songs",
                "operationId": "05c57eff8dc4c9012e55d00e4729857d",
                "responses": {
                    "200": {
                        "description": "Successful operation, Returns a list of songs in JSON format"
                    },
                    "401": {
                        "description": "Unauthenticated"
                    },
                    "403": {
                        "description": "Forbidden"
                    }
                }
            },
            "post": {
                "tags": [
                    "Songs"
                ],
                "summary": "Create a new song",
                "description": "Stores the song in the DB",
                "operationId": "store",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "title",
                                    "album",
                                    "artist",
                                    "genre",
                                    "rating"
                                ],
                                "properties": {
                                    "title": {
                                        "type": "string",
                                        "format": "string",
                                        "example": "Track Title"
                                    },
                                    "album": {
                                        "type": "string",
                                        "format": "string",
                                        "example": "Album Title"
                                    },
                                    "artist": {
                                        "type": "string",
                                        "format": "string",
                                        "example": "Artist's Name"
                                    },
                                    "genre": {
                                        "type": "string",
                                        "format": "string",
                                        "example": "Pop, Rock, Indie etc.."
                                    },
                                    "rating": {
                                        "type": "integer",
                                        "format": "integer",
                                        "example": "1-10"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Success",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "status": {
                                            "type": "integer",
                                            "example": ""
                                        },
                                        "data": {
                                            "type": "object"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/musics/{id}": {
            "get": {
                "tags": [
                    "Songs"
                ],
                "summary": "Display a song by it's {id}.",
                "description": "Gets a song by ID",
                "operationId": "b772ba56fdd4e28040aadc3ffc1a23ac",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "Music id",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful operation"
                    },
                    "401": {
                        "description": "Unauthenticated"
                    },
                    "403": {
                        "description": "Forbidden"
                    }
                }
            },
            "delete": {
                "tags": [
                    "Songs"
                ],
                "summary": "Delete Music",
                "description": "Delete Music",
                "operationId": "destroy",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "Id of a song",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "responses": {
                    "204": {
                        "description": "Success",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "status_code": {
                                            "type": "integer",
                                            "example": "204"
                                        },
                                        "data": {
                                            "type": "object"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            }
        }
    }
